<table class="table table-bordered table-hover table-striped" id="table_result">
    <thead>
        <tr>
            <td></td>
            <td>Id</td>
            <td>Password</td>
            <td>FullName</td>
            <td>Email</td>
            <td>Mobile</td>
            <td></td>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td><input type="checkbox" id="checkbox_@item.Id" /></td>
                <td>@item.Id</td>
                <td>@item.Password</td>
                <td>@item.FullName</td>
                <td>@item.Email</td>
                <td>@item.Mobile</td>
                <td>

                    <a href="/Procedure/Edit/@item.Id">Edit</a>
                    <a href="/Procedure/Delete/@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
<a class="btn btn-warning" href="/Checkox/Insert">Thêm sản phẩm</a>
<a class="btn btn-warning" href="">Chon sản phẩm</a>
