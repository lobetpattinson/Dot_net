
@{
    ViewBag.Title = "Script";
}

<h2>Script</h2>

<script src="/Ouput/Index8"></script>

@section scripts{
    <script>
        $(function () {
            $.ajax({
                url: "/Ouput/Index73",
                success: function (data) {
                    //alert(data[1].fullname);
                    for (var i = 0; i < data.length; i++) {
                        document.write("<ul>");
                        document.write("<li>" + data[i].fullname + "</li>");
                        document.write("<li>" + data[i].mark + "</li>");
                        document.write("</ul>");
                    }
                }
            });
        });
    </script>
}