<table class="table table-bordered table-condensed table-hover table-striped">
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>UnitPrice</td>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.UnitPrice</td>
            </tr>
        }
    </tbody>
</table>