
@{
    ViewBag.Title = "Numbers";
}

<h2>Numbers</h2>
<form action="/Regex/Numbers" method="post">
    <div class="form-group">
        <label>Dãy số</label>
        <input name="Numbers" class="form-control" />
    </div>
    <button class="btn btn-danger">OK</button>
</form>

<ul>
    @foreach (var n in ViewBag.EvenNumbers)
    {
        <li>@n</li>
    }
</ul>