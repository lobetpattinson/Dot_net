<table class="table table-bordered table-condensed table-hover table-striped">
    <thead>
        <tr>
            <td>Id</td>
            <td>Pass</td>
            <td>FullName</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Password</td>
                <td>@item.FullName</td>
            </tr>
        }
    </tbody>
</table>